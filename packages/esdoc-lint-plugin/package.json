{
  "name": "@enterthenamehere/esdoc-lint-plugin",
  "version": "2.1.0",
  "description": "A lint plugin for ESDoc",
  "author": "h13i32maru",
  "homepage": "https://github.com/EnterTheNameHere/esdoc-monorepo/tree/main/packages/esdoc-lint-plugin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EnterTheNameHere/esdoc-monorepo",
    "directory": "packages/esdoc-lint-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@enterthenamehere/esdoc-core": "file:../esdoc-core"
  },
  "engines": {
    "node": ">= 10.23.1"
  },
  "scripts": {
    "precoverage": "trash ./nyc_output && trash ./coverage",
    "coverage": "nyc npm run test",
    "pretest": "trash ./test/out",
    "test": "mocha --require ./test/init.js --recursive --extension .test.js"
  },
  "keywords": [
    "esdoc",
    "lint",
    "plugin"
  ],
  "files": [
    "src",
    "README.md"
  ],
  "directories": {
    "test": "test"
  },
  "main": "src/Plugin.js"
}
