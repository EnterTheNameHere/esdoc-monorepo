{
  "name": "@enterthenamehere/ice-cap",
  "version": "0.2.0-beta.0",
  "description": "Programmable DOM-based HTML template library.",
  "author": "h13i32maru",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EnterTheNameHere/esdoc-monorepo",
    "directory": "common/color-logger"
  },
  "scripts": {
    "build": "npm run clear && npm run transpile",
    "clear": "trash ./out",
    "esdoc": "esdoc -c esdoc.json",
    "prepare": "npm run build",
    "test": "npm run test:clear && npm run test:transpile && npm run test:mocha",
    "test:clear": "trash ./test/out",
    "test:mocha": "mocha ./test/out --recursive --extension .test.js --require esm -t 5000",
    "test:transpile": "babel ./test/src --out-dir ./test/out",
    "transpile": "babel ./src --out-dir ./out"
  },
  "dependencies": {
    "@enterthenamehere/color-logger": "^0.0.7-beta",
    "cheerio": "^1.0.0-rc.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@enterthenamehere/esdoc": "^1.3.0",
    "@enterthenamehere/esdoc-importpath-plugin": "^1.3.0",
    "eslint": "^7.14.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "esm": "^3.2.25",
    "fs-extra": "^9.0.1",
    "mocha": "^8.2.1",
    "trash-cli": "^4.0.0"
  },
  "keywords": [
    "html",
    "template"
  ],
  "files": [
    "out",
    "src"
  ],
  "main": "out/IceCap.js"
}
